{
  "name": "vite-project",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier": "prettier --write **/*.{js,jsx,tsx,ts,css,scss,md,json}",
    "eslint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "prepare": "husky install"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ],
    "*.{ts,tsx,js,jsx}": [
      "eslint src --ext .ts,.tsx,.js,.jsx --fix"
    ]
  },
  "dependencies": {
    "antd": "^4.19.3",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.2.2",
    "sass": "^1.50.0"
  },
  "devDependencies": {
    "@types/node": "^17.0.23",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^1.0.7",
    "eslint": "^8.13.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lint-staged": "^12.3.7",
    "prettier": "^2.6.2",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "yorkie": "^2.0.0"
  }
}
